[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
req_extensions     = req_ext
distinguished_name = dn

[ dn ]
CN = LocalChatServer

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1   = localhost
IP.1    = 127.0.0.1
IP.2    = 172.16.0.1
IP.3    = 172.31.255.255
IP.4    = 192.168.0.1
IP.5    = 192.168.255.255
IP.6    = 10.0.0.1
IP.7    = 10.255.255.255
